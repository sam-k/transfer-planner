    "eslint": "^8.56.0",
    "eslint-plugin-jsonc": "^2.12.2",
    "eslint-plugin-markdownlint": "^0.5.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^10.3.10",
